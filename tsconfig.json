{
	"compilerOptions": {
		"module": "es2022",
		"target": "es2024",
		"moduleResolution": "bundler",
		"paths": {
			"#*": [
				"./src/*"
			]
		},
		"esModuleInterop": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"declaration": true,
		"types": [
			"esbuild-style-plugin",
			"@wxn0brp/flanker-ui"
		]
	},
	"include": [
		"./src"
	],
	"exclude": [
		"node_modules"
	]
}